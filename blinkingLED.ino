
void setup() {
  pinMode(12, OUTPUT);
}

void loop() {
  digitalWrite(12, HIGH);   
  delay(100);              
  digitalWrite(12, LOW);
  delay(100);  
   digitalWrite(12, HIGH);   
  delay(300);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(500);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);  
  delay(700);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(900);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(1100);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(1300);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(1500);              
  digitalWrite(12, LOW);
  delay(100);
   digitalWrite(12, HIGH);   
  delay(1700);              
  digitalWrite(12, LOW);
  delay(100);            
}


